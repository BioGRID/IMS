<div class='pubBody'>
	<aside class='pubSidebar'>
	</aside>

	<div class='pubContent'>
		<div id='datasetDetails'>
			<div class='datasetLinkouts pull-right'>LINKOUTS {{ LINKOUTS }}</div>
			<h2 class='heading-line marginBotSm'>Dataset ({{ AVAILABILITY }})</h2>
			
			<div class='container-fluid marginBotSm {{ SHOW_WARNING }}'>
				<div class='alert alert-info marginTopSm marginBotNone text-center'>
					<strong><i class="fa fa-exclamation-circle fa-lg"></i> Warning! Danger!{{ ALERT_MESSAGE }}</strong>
				</div>
			</div>
			
			<h3>{{ TITLE }} ({{ DATASET_ID }})</h3>
			<p class='marginTopSm'><strong>{{ AUTHOR_LIST }}</strong></p>
			<p class='marginTopSm'>{{ ABSTRACT }}</p>
		</div>
		
		<div id='datasetProcessing'>
		</div>
	</div>
</div>